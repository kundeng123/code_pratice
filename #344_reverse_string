class Solution {
    public void reverseString(char[] s) {
    //time complexity O(n)
    //space o(1)
        //char[] result = new char[s.length];
        int len = s.length;
        for(int i=0; i<len/2; i++ ){
            char temp = s[i];
            s[i] = s[len-1-i];
            s[len-1-i] = temp;
        }
        //return result;
    }
}
